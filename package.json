{
  "name": "ts-morpheus",
  "version": "0.0.3",
  "description": "A TypeScript library that provides utilities for working with Angular code using ts-morph",
  "type": "module",
  "keywords": [
    "typescript",
    "angular",
    "ts-morph",
    "ast",
    "code-analysis",
    "refactoring"
  ],
  "author": "Evgeny Abramoich <evg.abramovitch@gmail.com>",
  "readme": "README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/evg4b/ts-morpheus.git"
  },
  "homepage": "https://evg4b.github.io/ts-morpheus/",
  "bugs": {
    "url": "https://github.com/evg4b/ts-morpheus/issues"
  },
  "exports": {
    "./common": {
      "types": "./dist/common.d.ts",
      "import": "./dist/common.js",
      "require": "./dist/common.cjs",
      "default": "./dist/common.js"
    },
    "./angular": {
      "types": "./dist/angular.d.ts",
      "import": "./dist/angular.js",
      "require": "./dist/angular.cjs",
      "default": "./dist/angular.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "test": "rstest",
    "lint": "eslint .",
    "docs": "typedoc --options typedoc.json"
  },
  "peerDependencies": {
    "ts-morph": "^27.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@eslint/js": "^9.35.0",
    "@microsoft/api-extractor": "^7.52.13",
    "@rslib/core": "^0.13.3",
    "@rstest/core": "^0.4.0",
    "@types/node": "^22.18.4",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "ts-morph": "^27.0.0",
    "typedoc": "^0.28.13",
    "typedoc-github-theme": "^0.3.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "packageManager": "yarn@4.9.4"
}
